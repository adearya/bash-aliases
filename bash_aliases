# Git
alias basename_xcc="basename '$(pwd)' | xclip -selection clipboard"
alias git_credential='git config --global credential.helper store'
alias git_readme='cp ~/SourceCode/git-readme/README $(pwd) && bash ~/SourceCode/git-readme/script'
alias git_new_repository='bash ~/SourceCode/git-new-repository/script'
alias xcc="xclip -selection clipboard"
